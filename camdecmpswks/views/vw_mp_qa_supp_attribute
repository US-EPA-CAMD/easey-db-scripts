CREATE OR REPLACE VIEW camdecmpswks.vw_mp_qa_supp_attribute
AS
 SELECT qsa.qa_supp_attribute_id, 
			ml.mon_plan_id, 
			qsd.mon_loc_id, 
			qsa.qa_supp_data_id, 
			qsa.attribute_name, 
			qsa.attribute_value
   FROM camdecmpswks.qa_supp_attribute AS qsa
     LEFT JOIN camdecmpswks.qa_supp_data qsd ON qsa.qa_supp_data_id = qsd.qa_supp_data_id
     LEFT JOIN camdecmpswks.vw_mp_location ml ON qsd.mon_loc_id = ml.mon_loc_id;

